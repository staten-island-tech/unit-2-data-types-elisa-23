def tipcalc():
    w = float(input("How much was your bill?"))
    y = input("How was your service?")
    tip = 0
    if (y == "good"):
        tip = int(w*0.18)
    elif (y == "excellent"):
        tip = int(w*0.22)
    elif (y == "bad"):
        tip = int(w*0.1)
    elif (y == "horrible"):
        tip = int(w*0.05)
    print(w+tip)
tipcalc()